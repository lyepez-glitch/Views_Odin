<!-- index.ejs -->
<html>
  <head>
    <title>Homepage</title>
    <link rel="stylesheet" href="styles.css">
  </head>
  <body>
   <h1>Home</h1>
<% if (users && users.length > 0) { %>
  <% users.forEach(function(user) { %>
    <div id="userCont">
      <div> <%= user %> </div>
    </div>
  <% }); %>
<% } else { %>
  <div>No users found.</div>
<% } %>


  </body>
</html>